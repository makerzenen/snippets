[user]
	name = Dustan Curtis
	email = 69445583+makerzenen@users.noreply.github.com
	username = makerzenen

[color]
	ui = auto

[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse

[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold

[diff]
	tool = vimdiff

[difftool]
	prompt = false

[alias]
	a = add
	b = branch
	bd = branch -D
	c = commit -m
	ca = commit --amend
	co = checkout
	empty = commit --allow-empty -m
	d = diff
	f = fetch
	fo = fetch origin
	g = grep -p
	l = log --pretty=oneline
	lg = log --oneline --graph --decorate
	m = merge
	p = push
	pf = push -f
	pt = push --tags
	pl = pull
	rb = rebase
	s = status
	t = tag
	aliases = !git config -l | grep alias | cut -c 7-
